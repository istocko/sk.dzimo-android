<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    android:contentDescription="@string/inspiracia_label"
    tools:context=".ui.priroda.PrirodaFragment">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="407dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/inspiracia_label"
        android:textAlignment="center"
        android:textColor="@color/red"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="130dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/inspiracia_label"
        android:src="@drawable/priroda1thumb"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="2dp" />

    <LinearLayout
        android:id="@+id/lay_234"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="150dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        tools:ignore="MissingConstraints"

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="120dp"
            android:layout_height="350dp"
            android:layout_margin="10px"
            android:contentDescription="@string/inspiracia_label"
            android:src="@drawable/priroda2thumb"
            app:layout_constraintTop_toBottomOf="@+id/imageView6"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="120dp"
            android:layout_height="350dp"
            android:layout_margin="10px"
            android:contentDescription="@string/inspiracia_label"
            android:src="@drawable/priroda3thumb"
            app:layout_constraintTop_toBottomOf="@+id/imageView6"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="120dp"
            android:layout_height="350dp"
            android:layout_margin="10px"
            android:contentDescription="@string/inspiracia_label"
            android:src="@drawable/priroda4thumb"
            app:layout_constraintTop_toBottomOf="@+id/imageView6"
            tools:ignore="MissingConstraints" />
    </LinearLayout>

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="130dp"
        android:layout_marginTop="-20dp"
        android:contentDescription="@string/inspiracia_label"
        android:src="@drawable/priroda5thumb"
        app:layout_constraintTop_toBottomOf="@+id/lay_234"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>